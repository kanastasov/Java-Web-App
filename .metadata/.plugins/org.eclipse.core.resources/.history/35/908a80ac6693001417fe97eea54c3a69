package test;

import beans.User;


public class Test {

	public static void main(String[] args) {

		User user = new User("L1087591@live.tees.ac.uk", "?letm?ein123!");
		if (user.validate()) {
			System.out.println("Bean validates OK. ");
		} else {
			System.out.println(user.getMessage());
		}
	}

}
